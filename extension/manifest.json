{
  "manifest_version": 3,
  "name": "IQ",
  "version": "1.0",
  "description": "A Chrome extension to display the profile with social url",
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "https://api.fullcontact.com/*"
  ],
  "icons": {
    "16": "assets/16x16 Icon.png",
    "19": "assets/19x19 Icon.png",
    "48": "assets/48x48 Icon.png",
    "128": "assets/128x128 Icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  }
}